{
  "name": "better-auth-elysia-prisma-example",
  "version": "1.0.0",
  "description": "Better Auth Studio example with Elysia and Prisma",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "studio": "pnpm exec better-auth-studio start",
    "studio:dev": "pnpm exec better-auth-studio start --port 3002 --config ./src/auth.ts",
    "studio:init": "pnpm exec better-auth-studio init",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.0",
    "@prisma/client": "^6.15.0",
    "better-auth": "^1.3.34",
    "better-auth-studio": "file:../../..",
    "elysia": "^1.1.0",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}

