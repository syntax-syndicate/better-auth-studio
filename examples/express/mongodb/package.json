{
  "name": "better-auth-express-mongodb-example",
  "version": "1.0.0",
  "description": "Express.js example with Better Auth and MongoDB",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "studio": "pnpm exec better-auth-studio start",
    "studio:dev": "pnpm exec better-auth-studio start --port 3002 --config ./src/auth.ts"
  },
  "dependencies": {
    "better-auth": "^1.3.34",
    "better-auth-studio": "file:../../..",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

