{
  "name": "better-auth-studio-monorepo",
  "version": "1.0.16",
  "description": "Better Auth Studio - Monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "studio:dev": "pnpm --filter better-auth-studio dev",
    "studio:build": "pnpm --filter better-auth-studio build",
    "cli:dev": "pnpm --filter better-auth-cli dev",
    "cli:build": "pnpm --filter better-auth-cli build",
    "web:dev": "pnpm --filter better-auth-web dev",
    "web:build": "pnpm --filter better-auth-web build",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "test:drizzle": "pnpm --filter test-drizzle dev",
    "test:prisma": "pnpm --filter test-prisma dev",
    "release": "pnpm build && bumpp && pnpm -r publish --access public --no-git-checks",
    "release:beta": "pnpm build && bumpp --preid beta && pnpm -r publish --access public --tag beta --no-git-checks",
    "release:canary": "pnpm build && bumpp --preid canary --no-commit --no-tag --no-push && pnpm -r publish --access public --tag canary --no-git-checks",
    "bump": "bumpp"
  },
  "keywords": [
    "better-auth",
    "studio",
    "dashboard",
    "authentication",
    "gui",
    "admin"
  ],
  "author": "kinfish",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "bumpp": "^10.2.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "packages/*/dist/",
    "packages/*/public/",
    "README.md",
    "package.json"
  ]
}
