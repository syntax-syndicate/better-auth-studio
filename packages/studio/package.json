{
  "name": "better-auth-studio",
  "version": "1.0.16",
  "description": "Studio for Better Auth",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && pnpm run build:ui",
    "build:ui": "cd ../ui && pnpm run build && mkdir -p ../studio/public && cp -r dist/* ../studio/public/",
    "dev": "tsx src/studio.ts",
    "clean": "rm -rf dist public",
    "ui:dev": "cd ../ui && pnpm run dev",
    "ui:build": "cd ../ui && pnpm run build",
    "ui:preview": "cd ../ui && pnpm run preview"
  },
  "keywords": [
    "better-auth",
    "studio",
    "dashboard",
    "authentication",
    "admin"
  ],
  "author": "kinfish",
  "license": "MIT",
  "dependencies": {
    "@types/commander": "^2.12.5",
    "better-auth": "1.3.5",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "jiti": "^2.5.1",
    "open": "^10.0.2",
    "postgres": "^3.4.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "public/",
    "README.md",
    "package.json"
  ]
}
